[user]
  email = kietnguyen86@gmail.com
  name = Kiet Nguyen
[alias]
  cp = cherry-pick
  st = status
  cl = clone
  ci = commit
  co = checkout
  br = branch
  lg = log --all --graph --abbrev-commit --decorate --date=format:'%Y-%m-%d %H:%M' --format=format:'%C(bold blue)%h%C(reset) - %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset) %C(green)[%ad]%C(reset)'
  unstage = 'reset HEAD --'
  last = 'log -1 HEAD'
  sl = stash list
  sa = stash apply
  ss = stash save
[color]
	ui = auto
[core]
	editor = vim
[push]
	default = upstream
[merge]
	conflictstyle = diff3
[credential]
	helper = cache --timeout=3600
